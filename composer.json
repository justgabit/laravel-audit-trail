{
    "name": "mueva/laravel-audit-trail",
    "description": "Laravel package to handle audit trails",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "^2.8@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mueva\\AuditTrail\\AuditTrailServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mueva\\AuditTrail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mueva\\AuditTrail\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.5",
        "limedeck/phpunit-detailed-printer": "~4.1",
        "mockery/mockery": "^1.0@dev",
        "mopo922/laravel-treats": "^2.0"
    }
}
